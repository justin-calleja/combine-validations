{
  "name": "@justinc/combine-validations",
  "version": "0.0.0-development",
  "description": "Provides a way to easily combine Folktale Validations",
  "main": "lib/index.js",
  "scripts": {
    "lint": "standard",
    "pretest": "npm run lint",
    "precommit": "npm t",
    "postcommit": "npm run readme",
    "prepush": "npm t",
    "commit": "git-cz",
    "commitmsg": "validate-commit-msg",
    "prereadme": "rimraf README.md",
    "readme": "jsdoc2md --template readmeTpl.hbs --files lib/* >> README.md",
    "test": "ava",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "keywords": [
    "combine",
    "validations",
    "folktale",
    "validation"
  ],
  "files": [
    "lib",
    "README.md"
  ],
  "author": "Justin Calleja <calleja.justin@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "data.validation": "1.3.1",
    "lodash.curry": "4.1.1"
  },
  "devDependencies": {
    "ava": "0.18.2",
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "2.0.0",
    "husky": "0.13.1",
    "jsdoc": "3.4.3",
    "jsdoc-to-markdown": "3.0.0",
    "rimraf": "2.6.0",
    "semantic-release": "6.3.2",
    "standard": "8.6.0",
    "validate-commit-msg": "^2.11.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/justin-calleja/combine-validations.git"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
